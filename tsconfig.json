{
  "compilerOptions": {
    "target": "ES2020",
    "useUnknownInCatchVariables": false,
    "lib": [
      "DOM",
      "ES2020"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": false
  }
}